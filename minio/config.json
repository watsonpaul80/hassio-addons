{
  "name": "MinIO",
  "slug": "minio",
  "description": "High performance, self-hosted S3 storage server for backups and object storage",
  "url": "https://github.com/watsonpaul80/hassio-addons/tree/main/minio",
  "startup": "services",
  "boot": "auto",
  "host_network": true,
  "privileged": [
    "NET_ADMIN",
    "SYS_ADMIN"
  ],
  "map": [
    "config:rw",
    "ssl",
    "addons:rw",
    "backup:rw",
    "share:rw",
    "media:rw"
  ],
  "options": {
    "generate_keys": true,
    "access_key": "",
    "secret_key": "",
    "port": 9000,
    "console_port": 9001
  },
  "schema": {
    "generate_keys": "bool",
    "access_key": "str?",
    "secret_key": "str?",
    "port": "int",
    "console_port": "int"
  },
  "arch": [
    "aarch64",
    "amd64"
  ],
  "image": "minio/minio:latest"
}
